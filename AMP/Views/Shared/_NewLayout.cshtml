    <link href="~/Content/Dashboard2/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Content/Dashboard2/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />

    <link href="~/Content/jquery.Jcrop.css" rel="stylesheet" />

    <!-- Custom Theme Style -->
    <link href="~/Content/Dashboard2/build/css/custom.min.css" rel="stylesheet">

    <!-- bootstrap-daterangepicker -->
    <link href="~/Content/Dashboard2/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- bootstrap-datetimepicker -->
    <link href="~/Content/Dashboard2/vendors/bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.css" rel="stylesheet">
    <!-- Bootstrap Colorpicker -->
    <link href="~/Content/Dashboard2/vendors/mjolnic-bootstrap-colorpicker/dist/css/bootstrap-colorpicker.min.css" rel="stylesheet">

