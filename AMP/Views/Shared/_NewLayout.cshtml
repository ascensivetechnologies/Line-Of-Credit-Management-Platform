<body class="nav-md">
   
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
               @Html.Partial("_SideLayout")
            </div>

            @Html.Partial("_TopLayout")
            <!-- page content -->
            <div class="right_col" role="main">
                @RenderBody()
                <a style="display:none;" class="btn btn-xs btn-default" id="download"></a>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <!-- jQuery -->
    <script src="~/Content/Dashboard2/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Content/Dashboard2/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/Dashboard2/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Content/Dashboard2/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="~/Content/Dashboard2/vendors/Chart.js/dist/Chart.min.js"></script>
